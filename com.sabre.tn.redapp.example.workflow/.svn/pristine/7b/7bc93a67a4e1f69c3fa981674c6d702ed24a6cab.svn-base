package com.sabre.tn.redapp.example.workflow.editor;

import org.eclipse.swt.browser.BrowserFunction;

import com.sabre.edge.platform.optional.browser.editors.BrowserEditor;

public class CustomBrowserEditor extends BrowserEditor {
	
	@Override
	public String getUrl() {
		return super.getUrl();
	}
	
	@Override
	public void navigate(String url) {
		new CustomBrowserFunction(browserBox.getBrowser());
		super.navigate(url);
		
		//browserBox.getBrowser().execute("alert('This page was opened from the Red App');");
	}
	

}
